

<div class = "media">
  <div class = "media-body">
    <table class = "table table-hover">
      <thead>
        <tr>
          <th>Venue</th>
          <th>Title</th>
          <th>Date</th>
          <th>Content</th>
          <th colspan="3"></th>
        </tr>
      </thead>

        <% @events.each do |event| %>
          <tr>
            <td><%= event.author %></td>
            <td><%= event.title %></td>
            <td><%= event.date %></td>
            <td><%= event.content %></td>
          </tr>
        <% end %>
      </tbody>

    </table>
  </div>
</div>
